{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:proxy\" \"npm run dev:live\" \"npm run dev:playlist\"",
    "dev:app": "vite",
    "dev:proxy": "tsx watch src/server/vod-proxy.ts",
    "dev:live": "tsx watch src/server/live-proxy.ts",
    "dev:playlist": "tsx watch src/server/playlist-proxy.ts",
    "dev:proxies": "tsx watch src/server/start-proxies.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "proxy:vod": "node src/server/vod-proxy.js",
    "proxy:live": "node src/server/live-proxy.js",
    "proxy:playlist": "node src/server/playlist-proxy.js",
    "proxy:all": "node src/server/start-proxies.js"
  },
